.scroll-list {
    overflow: hidden;
}

page {
    background: #f2f2f2;
}

.weui-icon-search, .weui-search-bar__text {
    vertical-align: inherit;
}

.topbar {
    background: #fff;
}

.topbar-item {
    color: #333;
    text-align: center;
    padding: 10px 0;
}

.topbar-item.active {
    color: #479de6;
}

.swiper_box {
    width: 100%;
}

swiper-item image {
    width: 100%;
    display: inline-block;
    overflow: hidden;
}

.weui-shop-type {
    width: 750rpx;
    height: auto;
    overflow: hidden;
    margin: 5rpx 0;
    background: #fff;
}

.weui-type-item {
    width: 150rpx;
    height: 150rpx;
    float: left;
    text-align: center;
    margin:10px 0px;
}

.weui-type-item-image {
    width: 80rpx;
    height: 80rpx;
    margin: 0 30rpx;
}

.weui-type-item-name {
    font-size: 12px;
    display: block;
    width: 100%;
    margin-top: 5px;
}

.goyhq{
  width: 750rpx;
  height: 250rpx;
}